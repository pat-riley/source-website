- var menu = { 'Overview': '/tour', 'Features': '/features', 'Enterprise': '/enterprise' };

.border-bottom-gray
  .row
    .column.medium-12
      ul.inline-list.subnav
        each val, key in menu
          li
            if tourSelected === key
              a.subnav-link.uppercase.active(href=val title=key)= key
            else
              a.subnav-link.uppercase(href=val title=key)= key
